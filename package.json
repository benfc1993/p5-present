{
  "name": "p5-present",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "rm -rf dist/assets && ./assetList.sh && cp -r public/ dist && yarn rollup -c --bundleConfigAsCjs",
    "build:server": "tsc ./src/server.ts --moduleResolution node --outDir dist",
    "client:dev": "ENVIRONMENT=DEV yarn build:client -w",
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "server:dev": "yarn build:server && concurrently \"yarn build:server -w\" \"nodemon ./dist/server.js\"",
    "server": "yarn build:server && node ./dist/server.js",
    "start": "ENVIRONMENT=PROD yarn build:client && yarn server"
  },
  "dependencies": {
    "browserify-fs": "^1.0.0",
    "concurrently": "^8.0.1",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "p5-typescript": "^2.4.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.35",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22",
    "rollup": "^3.15.0",
    "rollup-plugin-css": "^1.0.10",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-swc3": "^0.8.0",
    "rollup-plugin-ts": "^3.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.9.5"
  }
}
